package entities;

public class ElementoBliblioteca {
	
	long codIsbn;
	String titolo;
	int annoPubblicazione;
	int nPagine;

	public void Biblioteca(long codIsbn, String titolo, int annoPubblicazione, int nPagine) {
		this.codIsbn = codIsbn;
		this.titolo = titolo;
		this.annoPubblicazione = annoPubblicazione;
		this.nPagine = nPagine;
	}

}
