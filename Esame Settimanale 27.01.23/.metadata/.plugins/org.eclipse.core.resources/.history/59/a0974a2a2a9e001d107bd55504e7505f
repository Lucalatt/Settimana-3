package entities;

public class Libro extends ElementoBliblioteca {
	
	String autore;
	String genere;
	
	public Libro(long codiceIsbn, String titolo, int annoPubblicazione, int nPagine, String autore, String genere) {
		super(codiceIsbn, titolo, annoPubblicazione, nPagine);
		
		this.autore = autore;
		this.genere = genere;
	}

}
